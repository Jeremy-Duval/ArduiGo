<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ejb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD Enterprise JavaBeans 2.0//EN" "http://java.sun.com/dtd/ejb-jar_2_0.dtd">
<ejb-jar id="ejb-jar_1">
	<enterprise-beans>
		<entity id="ContainerManagedEntity_Item">
			<ejb-name>Item</ejb-name>
			<local-home>shop.ItemLocalHome</local-home>
			<local>shop.ItemLocal</local>
			<ejb-class>shop.ItemBean</ejb-class>
			<persistence-type>Container</persistence-type>
			<prim-key-class>shop.ItemPK</prim-key-class>
			<reentrant>False</reentrant>
			<cmp-version>2.x</cmp-version>
			<abstract-schema-name>Item</abstract-schema-name>
			<cmp-field id="CmpAttribute_Item_name">
				<field-name>name</field-name>
			</cmp-field>
			<cmp-field id="CmpAttribute_Item_price">
				<field-name>price</field-name>
			</cmp-field>
			<cmp-field id="CmpAttribute_Item_barcode">
				<field-name>barcode</field-name>
			</cmp-field>
			<query>
				<query-method>
					<method-name>findByPrice</method-name>
					<method-params>
						<method-param>java.lang.Double</method-param>
						<method-param>java.lang.Double</method-param>
					</method-params>
				</query-method>
				<ejb-ql/>
			</query>
			<home>shop.ItemHome</home>
			<remote>shop.Item</remote>
		</entity>
		<session id="Session_Cart">
			<ejb-name>Cart</ejb-name>
			<home>shop.CartHome</home>
			<remote>shop.Cart</remote>
			<ejb-class>shop.CartBean</ejb-class>
			<session-type>Stateless</session-type>
			<transaction-type>Container</transaction-type>
		</session>
	</enterprise-beans>
	<assembly-descriptor>
		<container-transaction>
			<method>
				<ejb-name>Item</ejb-name>
				<method-name>*</method-name>
			</method>
			<trans-attribute>Supports</trans-attribute>
		</container-transaction>
		<container-transaction>
			<method>
				<ejb-name>Cart</ejb-name>
				<method-name>*</method-name>
			</method>
			<trans-attribute>Required</trans-attribute>
		</container-transaction>
	</assembly-descriptor>
</ejb-jar>