<?xml version="1.0" encoding="UTF-8"?>
<Template>
	<TemplateInformationHeader name="Basic Information" description="/" type="table" default="true"/>
	<ElementBaseInitiationBlock>
	
		<StaticText content="Choreography Sub-Process : " style="Heading 1 (VP)"/>
		<StringPropertyText propertyText="name" style="Heading 1 (VP)"/>
		<ParagraphBreak/>
		<TableBlock colWidths="30, 70" tableWidth="15500" alignment="right">
			<ValueConditionChecker property="userID" operator="NOT EQUAL" value="">
				<TableRow>
					<TableCell leftBorderEnable="false" rightBorderEnable="false">
						<StaticText content="ID :" style="Row caption 1"/>
					</TableCell>
					<TableCell leftBorderEnable="false" rightBorderEnable="false">
						<StringPropertyText propertyText="userID" style="Table Contents"/>
					</TableCell>
				</TableRow>
			</ValueConditionChecker>
			<HasMetaModelPropertyChecker property="stereotypes" flag="true">
				<TableRow>
					<TableCell leftBorderEnable="false" rightBorderEnable="false">
						<StaticText content="Stereotypes :" style="Row caption 1"/>
					</TableCell>
					<TableCell leftBorderEnable="false" rightBorderEnable="false">
						<ForEach property="stereotypes" ignoreParagraphBreakForLastModel="true">
							<StaticText content="&lt;&lt;" style="Table Contents"/>
							<StringPropertyText propertyText="name" style="Table Contents"/>
							<StaticText content=">>" style="Table Contents"/>
							<StaticText content="\n" style="Table Contents"/>
						</ForEach>
					</TableCell>
				</TableRow>
			</HasMetaModelPropertyChecker>
			<ValueConditionChecker property="documentation" operator="NOT EQUAL" value="">
				<ReportOptionChecker option="basicInformationShowsDocumentation" equalFlag="true">
					<TableRow>
						<TableCell leftBorderEnable="false" rightBorderEnable="false">
							<StaticText content="Doc :" style="Row caption 1"/>
						</TableCell>
						<TableCell leftBorderEnable="false" rightBorderEnable="false">
							<StringPropertyText propertyText="documentation" style="Table Contents"/>
						</TableCell>
					</TableRow>
				</ReportOptionChecker>
			</ValueConditionChecker>
			<HasMetaModelPropertyChecker property="participants" flag="true">
				<TableRow>
					<TableCell leftBorderEnable="false" rightBorderEnable="false">
						<StaticText content="Participants :" style="Row caption 1"/>
					</TableCell>
					<TableCell leftBorderEnable="false" rightBorderEnable="false">
						<ForEach property="participants" ignoreParagraphBreakForLastModel="true">
							<StringPropertyText propertyText="value" style="Table Contents"/>
							<StaticText content="\n" style="Table Contents"/>
						</ForEach>
					</TableCell>
				</TableRow>
			</HasMetaModelPropertyChecker>
			<HasMetaModelPropertyChecker property="initiatingParticipant" flag="true">
				<MetaModelElement modelProperty="initiatingParticipant">
					<TableRow>
						<TableCell leftBorderEnable="false" rightBorderEnable="false">
							<StaticText content="Initiating Participant :" style="Row caption 1"/>
						</TableCell>
						<TableCell leftBorderEnable="false" rightBorderEnable="false">
							<StringPropertyText propertyText="name" style="Table Contents"/>
						</TableCell>
					</TableRow>
				</MetaModelElement>
			</HasMetaModelPropertyChecker>
			<HasMetaModelPropertyChecker property="loopType" flag="true">
				<MetaModelElement modelProperty="loopType">
					<TableRow>
						<TableCell leftBorderEnable="false" rightBorderEnable="false">
							<StaticText content="Loop Type :" style="Row caption 1"/>
						</TableCell>
						<TableCell leftBorderEnable="false" rightBorderEnable="false">
							<StringPropertyText propertyText="name" style="Table Contents"/>
							<StaticText content=":" style="Table Contents"/>
							<StringPropertyText propertyText="modelType" style="Table Contents"/>
						</TableCell>
					</TableRow>
				</MetaModelElement>
			</HasMetaModelPropertyChecker>
		</TableBlock>
		<StaticText content="" style="Default"/>
	</ElementBaseInitiationBlock>
</Template>