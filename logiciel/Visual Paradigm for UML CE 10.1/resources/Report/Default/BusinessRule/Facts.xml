<?xml version="1.0" encoding="UTF-8"?>
<Template>
	<TemplateInformationHeader name="Facts" description="/" type="table" default="false"/>
	<ElementBaseInitiationBlock>

		<StaticText content="Facts" style="Heading 2 (VP)"/>
		<ParagraphBreak/>
		<HasMetaModelPropertyChecker property="facts" flag="true">
			<TableBlock colWidths="30, 70" tableWidth="14500" alignment="right">
				<TableRow>
					<TableCell leftBorderEnable="false" rightBorderEnable="false" color="230, 230, 230">
						<StaticText content="Name" style="Column header 1"/>
					</TableCell>
					<TableCell leftBorderEnable="false" rightBorderEnable="false" color="230, 230, 230">
						<StaticText content="Definition" style="Column header 1"/>
					</TableCell>
				</TableRow>
				<ForEach property="facts">
					<TableRow>
						<TableCell leftBorderEnable="false" rightBorderEnable="false">
							<StringPropertyText property="name" style="Table Contents"/>
						</TableCell>
						<TableCell leftBorderEnable="false" rightBorderEnable="false">
							<StringPropertyText property="definition" style="Table Contents"/>
						</TableCell>
					</TableRow>
				</ForEach>
			</TableBlock>
		</HasMetaModelPropertyChecker>
		<StaticText content="" style="Default"/>
	</ElementBaseInitiationBlock>
</Template>