<?xml version="1.0" encoding="UTF-8"?>
<Template>
	<TemplateInformationHeader name = "Basic Information" description = "/" type = "table" default = "true"/>
	<ElementBaseInitiationBlock>
		
		<StaticText content = "CRC Card : " style = "Heading 1 (VP)"/>
		<StringPropertyText propertyText = "name" style = "Heading 1 (VP)"/>
		<ParagraphBreak/>
		<TableBlock colWidths = "20, 80" tableWidth = "15500" alignment = "right">
			<ValueConditionChecker property = "name" operator = "NOT EQUAL" value = "">
				<TableRow>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
						<StaticText content = "Class name :" style = "Row caption 1"/>
					</TableCell>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
						<StringPropertyText propertyText = "name" style = "Table Contents"/>
					</TableCell>
				</TableRow>
			</ValueConditionChecker>
			<ValueConditionChecker property = "superClasses" operator = "NOT EQUAL" value = "">
				<TableRow>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
						<StaticText content = "Super Classes :" style = "Row caption 1"/>
					</TableCell>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
						<StringPropertyText propertyText = "superClasses" style = "Table Contents"/>
					</TableCell>
				</TableRow>
			</ValueConditionChecker>
			<ValueConditionChecker property = "subClasses" operator = "NOT EQUAL" value = "">
				<TableRow>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
						<StaticText content = "Sub Classes :" style = "Row caption 1"/>
					</TableCell>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
						<StringPropertyText propertyText = "subClasses" style = "Table Contents"/>
					</TableCell>
				</TableRow>
			</ValueConditionChecker>
			<ValueConditionChecker property = "documentation" operator = "NOT EQUAL" value = "">
				<ReportOptionChecker option = "basicInformationShowsDocumentation" equalFlag = "true">
					<TableRow>
						<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
							<StaticText content = "Doc :" style = "Row caption 1"/>
						</TableCell>
						<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
							<StringPropertyText propertyText = "documentation" style = "Table Contents"/>
						</TableCell>
					</TableRow>
				</ReportOptionChecker>
			</ValueConditionChecker>
			<HasChildElementChecker type = "CRCAttribute" flag = "true">
				<TableRow>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
						<StaticText content = "CRC Attribute :" style = "Row caption 1"/>
					</TableCell>
					<TableCell splitted = "true">
						<TableRow>
							<TableCell leftBorderEnable = "false" rightBorderEnable = "false" color = "230, 230, 230">
								<StaticText content = "Name :" style = "Sub-column header 1"/>
							</TableCell>
							<TableCell leftBorderEnable = "false" rightBorderEnable = "false" color = "230, 230, 230">
								<StaticText content = "Description :" style = "Sub-column header 1"/>
							</TableCell>
						</TableRow>
						<IterationBlock elementType = "CRCAttribute">
							<TableRow>
								<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
									<StringPropertyText propertyText = "name" style = "Table Contents"/>
								</TableCell>
								<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
									<StringPropertyText propertyText = "documentation" style = "Table Contents"/>
								</TableCell>
							</TableRow>
						</IterationBlock>
					</TableCell>
				</TableRow>
			</HasChildElementChecker>
			<HasChildElementChecker type = "CRCResponsibility" flag = "true">
				<TableRow>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
						<StaticText content = "CRC Responsibility :" style = "Row caption 1"/>
					</TableCell>
					<TableCell splitted = "true">
						<TableRow>
							<TableCell leftBorderEnable = "false" rightBorderEnable = "false" color = "230, 230, 230">
								<StaticText content = "Name :" style = "Sub-column header 1"/>
							</TableCell>
							<TableCell leftBorderEnable = "false" rightBorderEnable = "false" color = "230, 230, 230">
								<StaticText content = "Collaborator :" style = "Sub-column header 1"/>
							</TableCell>
						</TableRow>
						<IterationBlock elementType = "CRCResponsibility">
							<TableRow>
								<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
									<StringPropertyText propertyText = "name" style = "Table Contents"/>
								</TableCell>
								<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
									<StringPropertyText propertyText = "collaborator" style = "Table Contents"/>
								</TableCell>
							</TableRow>
						</IterationBlock>
					</TableCell>
				</TableRow>
			</HasChildElementChecker>
			
		</TableBlock>
		<StaticText content = "" style = "Default"/>
	</ElementBaseInitiationBlock>
</Template>