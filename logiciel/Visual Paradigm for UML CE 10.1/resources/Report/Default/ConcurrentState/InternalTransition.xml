<?xml version="1.0" encoding="UTF-8"?>
<Template>
	<TemplateInformationHeader name = "Internal Transition" description = "/" type = "table" default = "false"/>
	<ElementBaseInitiationBlock>
	
		<StaticText content = "Internal Transition" style = "Heading 2 (VP)"/>
		<ParagraphBreak/>
		<HasMetaModelPropertyChecker property = "internalTransitions" flag = "true">
			<TableBlock colWidths = "2500,2500,2500,2500" tableWidth = "14500" alignment = "right">
				<TableRow>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false" color = "230, 230, 230">
						<StaticText content = "Name" style = "Column header 1"/>
					</TableCell>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false" color = "230, 230, 230">
						<StaticText content = "Parameter" style = "Column header 1"/>
					</TableCell>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false" color = "230, 230, 230">
						<StaticText content = "Guard Condition" style = "Column header 1"/>
					</TableCell>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false" color = "230, 230, 230">
						<StaticText content = "Expression" style = "Column header 1"/>
					</TableCell>
				</TableRow>
				<MetaModelElement modelProperty = "internalTransitions">
					<TableRow>
						<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
							<StringPropertyText propertyText = "name" style = "Table Contents"/>
						</TableCell>
						<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
							<IterationBlock elementType = "Parameter">
								<StringPropertyText propertyText = "name" style = "Table Contents"/>
								<StaticText content = " : " style = "Table Contents"/>
								<StringPropertyText propertyText = "type" style = "Table Contents"/>
							</IterationBlock>
						</TableCell>
						<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
							<StringPropertyText propertyText = "guardCondition" style = "Table Contents"/>
						</TableCell>
						<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
							<StringPropertyText propertyText = "expression" style = "Table Contents"/>
						</TableCell>
					</TableRow>
				</MetaModelElement>
			</TableBlock>
		</HasMetaModelPropertyChecker>
		<StaticText content = "" style = "Default"/>
	</ElementBaseInitiationBlock>
</Template>