<?xml version="1.0" encoding="UTF-8"?>
<Template>
	<TemplateInformationHeader name="Message Flows" description="/" type="table" default="false"/>
	<ElementBaseInitiationBlock>
	
		<StaticText content="Message Flows" style="Heading 2 (VP)"/>
		<ParagraphBreak/>
		
		<HasMetaModelPropertyChecker property="messageFlows" flag="true">
			<TableBlock colWidths="30, 35, 35" tableWidth="14500" alignment="right">
				<TableRow>
					<TableCell leftBorderEnable="false" rightBorderEnable="false" color="230, 230, 230">
						<StaticText content="Name" style="Column header 1"/>
					</TableCell>
					<TableCell leftBorderEnable="false" rightBorderEnable="false" color="230, 230, 230">
						<StaticText content="From" style="Column header 1"/>
					</TableCell>
					<TableCell leftBorderEnable="false" rightBorderEnable="false" color="230, 230, 230">
						<StaticText content="To" style="Column header 1"/>
					</TableCell>
				</TableRow>
				<ForEach property="messageFlows">
					<TableRow>
						<TableCell leftBorderEnable="false" rightBorderEnable="false">
							<StringPropertyText propertyText="name" style="Table Contents"/>
						</TableCell>
						<TableCell leftBorderEnable="false" rightBorderEnable="false">
							<StringPropertyText propertyText="from" style="Table Contents"/>
						</TableCell>
						<TableCell leftBorderEnable="false" rightBorderEnable="false">
							<StringPropertyText propertyText="to" style="Table Contents"/>
						</TableCell>
					</TableRow>
				</ForEach>
			</TableBlock>
		</HasMetaModelPropertyChecker>
		<StaticText content="" style="Default"/>
	</ElementBaseInitiationBlock>
</Template>