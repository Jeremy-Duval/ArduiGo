<?xml version="1.0" encoding="UTF-8"?>
<Template>
	<TemplateInformationHeader name = "Constraints" description = "/" type = "table" default = "false"/>
	<ElementBaseInitiationBlock>

		<StaticText content = "Constraints" style = "Heading 2 (VP)"/>
		<ParagraphBreak/>
		<HasMetaModelPropertyChecker property = "constraints" flag= "true">
			<TableBlock colWidths = "5000,5000" tableWidth = "14500" alignment = "right">
				<TableRow>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false" color = "230, 230, 230">
						<StaticText content = "Name" isBold = "true" fontSize = "10" isItalic = "true" alignment = "center"/>
					</TableCell>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false" color = "230, 230, 230">
						<StaticText content = "Type" isBold = "true" fontSize = "10" isItalic = "true" alignment = "center"/>
					</TableCell>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false" color = "230, 230, 230">
						<StaticText content = "Default Value" isBold = "true" fontSize = "10" isItalic = "true" alignment = "center"/>
					</TableCell>
				</TableRow>
				<ForEach property = "constraints">
					<TableRow>
						<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
							<StringPropertyText propertyText = "name" style = "Table Contents"/>
						</TableCell>
						<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
							<ValueConditionChecker property = "modelType" operator = "EQUAL" value = "DBCheckConstraint">
								<StaticText content = "Check" style = "Table Contents"/>
							</ValueConditionChecker>
							<ValueConditionChecker property = "modelType" operator = "EQUAL" value = "DBUniqueConstraint">
								<StaticText content = "Unique" style = "Table Contents"/>
							</ValueConditionChecker>
						</TableCell>
						<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
							<ValueConditionChecker property = "modelType" operator = "EQUAL" value = "DBCheckConstraint">
								<StringPropertyText propertyText = "checkConstraint" style = "Table Contents"/>
							</ValueConditionChecker>
							<ValueConditionChecker property = "modelType" operator = "EQUAL" value = "DBUniqueConstraint">
								<HasMetaModelPropertyChecker property = "columns" flag = "true">
									<StaticText content = "UNIQUE(" style = "Table Contents"/>
									<ForEach property = "columns" ignoreParagraphBreakForLastModel = "true">
										<StringPropertyText propertyText = "name" style = "Table Contents"/>
										<StaticText content = ", " style = "Table Contents"/>
									</ForEach>
									<StaticText content = ")" style = "Table Contents"/>
								</HasMetaModelPropertyChecker>
							</ValueConditionChecker>
						</TableCell>
					</TableRow>
				</ForEach>
			</TableBlock>
		</HasMetaModelPropertyChecker>
		<StaticText content = "" style = "Default"/>
	</ElementBaseInitiationBlock>
</Template>