<?xml version="1.0" encoding="UTF-8"?>
<Template>
	<TemplateInformationHeader name = "Columns Detail" description = "/" type = "table" default = "false"/>
	<ElementBaseInitiationBlock>
	
		<StaticText content = "Columns" style = "Heading 2 (VP)"/>
		<ParagraphBreak/>
		<HasChildElementChecker type = "DBColumn" flag = "true">
			<IterationBlock elementType = "DBColumn">
				<StringPropertyText propertyText = "name" style = "Heading 3 (VP)"/>
				<ParagraphBreak/>
				<TableBlock colWidths = "3000,11500" tableWidth = "14500" alignment = "right">
					<TableRow>
						<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
							<StaticText content = "Name :" style = "Row caption 1"/>
						</TableCell>
						<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
							<StringPropertyText propertyText = "name" style = "Table Contents"/>
						</TableCell>
					</TableRow>
					<TableRow>
						<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
							<StaticText content = "Type :" style = "Row caption 1"/>
						</TableCell>
						<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
							<StringPropertyText propertyText = "dbColumnType" style = "Table Contents"/>
						</TableCell>
					</TableRow>
					<TableRow>
						<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
							<StaticText content = "Length :" style = "Row caption 1"/>
						</TableCell>
						<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
							<StringPropertyText propertyText = "length" style = "Table Contents"/>
						</TableCell>
					</TableRow>
					<TableRow>
						<TableCell leftBorderEnable="false" rightBorderEnable="false">
							<StaticText content="Scale :" style="Row caption 1"/>
						</TableCell>
						<TableCell leftBorderEnable="false" rightBorderEnable="false">
							<StringPropertyText property="scale" style="Table Contents"/>
						</TableCell>
					</TableRow>
					<TableRow>
						<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
							<StaticText content = "Primary Key :" style = "Row caption 1"/>
						</TableCell>
						<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
							<ValueConditionChecker property = "primaryKey" operator = "EQUAL" value = "true">
								<StaticText content = "true" style = "Table Contents"/>
							</ValueConditionChecker>
							<ValueConditionChecker property = "primaryKey" operator = "EQUAL" value = "false">
								<StaticText content = "false" style = "Table Contents"/>
							</ValueConditionChecker>
						</TableCell>
					</TableRow>
					<TableRow>
						<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
							<StaticText content = "Foreign Key :" style = "Row caption 1"/>
						</TableCell>
						<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
							<ValueConditionChecker property = "foreignKey" operator = "EQUAL" value = "true">
								<StaticText content = "true" style = "Table Contents"/>
							</ValueConditionChecker>
							<ValueConditionChecker property = "foreignKey" operator = "EQUAL" value = "false">
								<StaticText content = "false" style = "Table Contents"/>
							</ValueConditionChecker>
						</TableCell>
					</TableRow>
					<TableRow>
						<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
							<StaticText content = "Nullable :" style = "Row caption 1"/>
						</TableCell>
						<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
							<StringPropertyText propertyText = "nullable" style = "Table Contents"/>
						</TableCell>
					</TableRow>
					<TableRow>
						<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
							<StaticText content = "Unique :" style = "Row caption 1"/>
						</TableCell>
						<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
							<StringPropertyText propertyText = "unique" style = "Table Contents"/>
						</TableCell>
					</TableRow>
					<TableRow>
						<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
							<StaticText content = "Index :" style = "Row caption 1"/>
						</TableCell>
						<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
							<StringPropertyText propertyText = "index" style = "Table Contents"/>
						</TableCell>
					</TableRow>
				</TableBlock>
				<ParagraphBreak/>
				<StaticText content = "" style = "Default"/>
				<ParagraphBreak/>
				<ValueConditionChecker property = "documentation" operator = "NOT EQUAL" value = "">
					<StaticText indentation = "2150" content = "Documentation"  style = "DefaultBold"/>
					<ParagraphBreak/>
					<StringPropertyText indentation = "2150" forcePlainText = "true" propertyText = "documentation" style = "Table Contents"/>
					<ParagraphBreak/>
				</ValueConditionChecker>
				<StaticText content = "" style = "Default"/>
			</IterationBlock>
		</HasChildElementChecker>
	</ElementBaseInitiationBlock>
</Template>