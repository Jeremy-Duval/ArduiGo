<?xml version="1.0" encoding="UTF-8"?>
<Template>
	<TemplateInformationHeader name="Relationships" description="/" type="table" default="false"/>
	<ElementBaseInitiationBlock>

		<StaticText content="Relationships" style="Heading 2 (VP)"/>
		<ParagraphBreak/>
		<ForEachSimpleRelationship type="DBForeignKey" direction="all" ignoreParagraphBreakForLastModel="true">
			<TableBlock colWidths="20, 20, 60" tableWidth="14500" alignment="right">
				<TableRow>
					<TableCell leftBorderEnable="false" rightBorderEnable="false" color="230, 230, 230">
						<StaticText content="Name :" style="Row caption 1"/>
					</TableCell>
					<TableCell leftBorderEnable="false" rightBorderEnable="false" color="230, 230, 230" colspan="2">
						<StringPropertyText property="name" style="Table Contents"/>
					</TableCell>
				</TableRow>
				
				<!-- From -->
				<!-- since the rowspan is fixed. Cannot hide the TableRow of Phrasse/Cardinate" even they are empty -->
				<TableRow>
					<TableCell leftBorderEnable="false" rightBorderEnable="false" rowspan="3">
						<StaticText content="From :" style="Row caption 1"/>
					</TableCell>
					<TableCell leftBorderEnable="false" rightBorderEnable="false">
						<StaticText content="Name :" style="Row caption 1"/>
					</TableCell>
					<TableCell leftBorderEnable="false" rightBorderEnable="false">
						<FromEnd>
							<StringPropertyText property="name" style="Table Contents"/>
						</FromEnd>
					</TableCell>
				</TableRow>
				<TableRow>
					<TableCell leftBorderEnable="false" rightBorderEnable="false">
						<StaticText content="Phrase :" style="Row caption 1"/>
					</TableCell>
					<TableCell leftBorderEnable="false" rightBorderEnable="false">
						<StringPropertyText property="fromPhrase" style="Table Contents"/>
					</TableCell>
				</TableRow>
				<TableRow>
					<TableCell leftBorderEnable="false" rightBorderEnable="false">
						<StaticText content="Cardinality :" style="Row caption 1"/>
					</TableCell>
					<TableCell leftBorderEnable="false" rightBorderEnable="false">
						<StringPropertyText property="fromMultiplicity" style="Table Contents"/>
					</TableCell>
				</TableRow>
				
				<!-- To -->
				<!-- since the rowspan is fixed. Cannot hide the TableRow of Phrasse/Cardinate" even they are empty -->
				<TableRow>
					<TableCell leftBorderEnable="false" rightBorderEnable="false" rowspan="3">
						<StaticText content="To :" style="Row caption 1"/>
					</TableCell>
					<TableCell leftBorderEnable="false" rightBorderEnable="false">
						<StaticText content="Name :" style="Row caption 1"/>
					</TableCell>
					<TableCell leftBorderEnable="false" rightBorderEnable="false">
						<ToEnd>
							<StringPropertyText property="name" style="Table Contents"/>
						</ToEnd>
					</TableCell>
				</TableRow>
				<TableRow>
					<TableCell leftBorderEnable="false" rightBorderEnable="false">
						<StaticText content="Phrase :" style="Row caption 1"/>
					</TableCell>
					<TableCell leftBorderEnable="false" rightBorderEnable="false">
						<StringPropertyText property="toPhrase" style="Table Contents"/>
					</TableCell>
				</TableRow>
				<TableRow>
					<TableCell leftBorderEnable="false" rightBorderEnable="false">
						<StaticText content="Cardinality :" style="Row caption 1"/>
					</TableCell>
					<TableCell leftBorderEnable="false" rightBorderEnable="false">
						<StringPropertyText property="toMultiplicity" style="Table Contents"/>
					</TableCell>
				</TableRow>
				
			</TableBlock>
				
			<!-- Foreign Key Constraints -->
			<HasMetaModelPropertyChecker property="foreignKeyConstraints">
				
				<TableBlock colWidths="50, 50" tableWidth="14500" alignment="right">
					<TableRow>
						<TableCell leftBorderEnable="false" rightBorderEnable="false" topBorderEnable="false" color="230, 230, 230" colspan="2">
							<StaticText content="Foreign Key Column Mapping" style="Row caption 1"/>
						</TableCell>
					</TableRow>
					<TableRow>
						<TableCell leftBorderEnable="false" rightBorderEnable="false" color="230, 230, 230">
							<StaticText content="Primary Key (" style="Row caption 1"/>
							<FromEnd>
								<StringPropertyText property="name" style="Table Contents"/>
							</FromEnd>
							<StaticText content=")" style="Row caption 1"/>
						</TableCell>
						<TableCell leftBorderEnable="false" rightBorderEnable="false" color="230, 230, 230">
							<StaticText content="Foreign Key (" style="Row caption 1"/>
							<ToEnd>
								<StringPropertyText property="name" style="Table Contents"/>
							</ToEnd>
							<StaticText content=")" style="Row caption 1"/>
						</TableCell>
					</TableRow>
					
					<ForEach property="foreignKeyConstraints">
						<TableRow>
							<TableCell leftBorderEnable="false" rightBorderEnable="false">
								<MetaModelElement property="pkColumn">
									<StringPropertyText property="name" style="Table Contents"/>
								</MetaModelElement>
							</TableCell>
							<TableCell leftBorderEnable="false" rightBorderEnable="false">
								<MetaModelElement property="fkColumn">
									<StringPropertyText property="name" style="Table Contents"/>
								</MetaModelElement>
							</TableCell>
						</TableRow>
					</ForEach>
				</TableBlock>
			</HasMetaModelPropertyChecker>
			
			<ParagraphBreak/>
			<StaticText content="" style="Default"/>
			<ParagraphBreak/>
			
		</ForEachSimpleRelationship>
		<StaticText content="" style="Default"/>
	</ElementBaseInitiationBlock>
</Template>