<?xml version="1.0" encoding="UTF-8"?>
<Template>
	<TemplateInformationHeader name="Basic Information" description="/" type="table" default="true"/>
	<ElementBaseInitiationBlock>
	
		<StaticText content="Term : " style="Heading 1 (VP)"/>
		<StringPropertyText property="name" style="Heading 1 (VP)"/>
		<ParagraphBreak/>
		<TableBlock colWidths = "20, 80" tableWidth="15500" alignment="right">
			<HasMetaModelPropertyChecker property="aliases" flag="true">
				<TableRow>
					<TableCell leftBorderEnable="false" rightBorderEnable="false">
						<StaticText content="Aliases :" style="Row caption 1"/>
					</TableCell>
					<TableCell leftBorderEnable="false" rightBorderEnable="false">
						<ForEach property="aliases" ignoreParagraphBreakForLastModel="true">
							<StringPropertyText property="name" style="Table Contents"/>
							<StaticText content="\n" style="Table Contents"/>
						</ForEach>
					</TableCell>
				</TableRow>
			</HasMetaModelPropertyChecker>
			<HasMetaModelPropertyChecker property="labels" flag="true">
				<TableRow>
					<TableCell leftBorderEnable="false" rightBorderEnable="false">
						<StaticText content="Labels :" style="Row caption 1"/>
					</TableCell>
					<TableCell leftBorderEnable="false" rightBorderEnable="false">
						<ForEach property="labels" ignoreParagraphBreakForLastModel="true">
							<StringPropertyText property="name" style="Table Contents"/>
							<StaticText content="\n" style="Table Contents"/>
						</ForEach>
					</TableCell>
				</TableRow>
			</HasMetaModelPropertyChecker>
			<ValueConditionChecker property="definition" operator="not equals" value="">
				<TableRow>
					<TableCell leftBorderEnable="false" rightBorderEnable="false">
						<StaticText content="Definition :" style="Row caption 1"/>
					</TableCell>
					<TableCell leftBorderEnable="false" rightBorderEnable="false">
						<StringPropertyText property="definition" style="Table Contents"/>
					</TableCell>
				</TableRow>
			</ValueConditionChecker>
			<ValueConditionChecker property="documentation" operator="NOT EQUAL" value="">
				<ReportOptionChecker option="basicInformationShowsDocumentation" equalFlag="true">
					<TableRow>
						<TableCell leftBorderEnable="false" rightBorderEnable="false">
							<StaticText content="Doc :" style="Row caption 1"/>
						</TableCell>
						<TableCell leftBorderEnable="false" rightBorderEnable="false">
							<StringPropertyText property="documentation" style="Table Contents"/>
						</TableCell>
					</TableRow>
				</ReportOptionChecker>
			</ValueConditionChecker>
		</TableBlock>
		<StaticText content="" style="Default"/>
	</ElementBaseInitiationBlock>
</Template>