<?xml version="1.0" encoding="UTF-8"?>
<Template>
	<TemplateInformationHeader name = "Attribute Links" description = "/" type = "table" default = "false"/>
	<ElementBaseInitiationBlock>
	
		<StaticText content = "Attribute Links" style = "Heading 2 (VP)"/>
		<ParagraphBreak/>
		<HasMetaModelPropertyChecker property = "attributeLinks" flag= "true">
			<TableBlock colWidths = "3333,3333,3333" tableWidth = "14500" alignment= "right">
				<TableRow>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false" color = "230, 230, 230">
						<StaticText content = "Attribute name" style = "Column header 1"/>
					</TableCell>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false" color = "230, 230, 230">
						<StaticText content = "Type" style = "Column header 1"/>
					</TableCell>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false" color = "230, 230, 230">
						<StaticText content = "Initial Value" style = "Column header 1"/>
					</TableCell>
				</TableRow>
				<ForEach property = "attributeLinks">
					<TableRow>
						<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
							<MetaModelElement modelProperty = "attribute">
								<StringPropertyText propertyText = "name" style = "Table Contents"/>
							</MetaModelElement>
						</TableCell>
						<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
							<MetaModelElement modelProperty = "attribute">
								<StringPropertyText propertyText = "type" style = "Table Contents"/>
							</MetaModelElement>
						</TableCell>
						<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
							<StringPropertyText propertyText = "initialValue" style = "Table Contents"/>
						</TableCell>
					</TableRow>
				</ForEach>
			</TableBlock>
		</HasMetaModelPropertyChecker>
		<StaticText content = "" style = "Default"/>
	</ElementBaseInitiationBlock>
</Template>