<Template>
	<TemplateInformationHeader name = "Requirement" description = "/" type = "text" default = "false"/>
	<ElementBaseInitiationBlock>
		
		<ElementImage imageType="icon"/>
		<StringPropertyText propertyText="name" style = "Heading 1"/>
		<ValueConditionChecker property="userID" operator="not equals" value="">
			<StaticText content=" (" style = "Heading 1"/>
			<StringPropertyText propertyText="userID" style = "Heading 1"/>
			<StaticText content=")" style = "Heading 1"/>
		</ValueConditionChecker> 
		<ParagraphBreak/>
		
		<ValueConditionChecker property="documentation" operator="not equals" value="">
			<StringPropertyText propertyText="documentation"/>
			<ParagraphBreak/>
		</ValueConditionChecker>
	
		<!-- attributes -->
		<ValueConditionChecker property = "source" operator = "NOT EQUAL" value = "">
			<StaticText content = "Source :" style = "Heading 3"/>
			<StringPropertyText propertyText = "source"/>
			<ParagraphBreak/>
		</ValueConditionChecker>
		<ValueConditionChecker property = "kind" operator = "NOT EQUAL" value = "">
			<StaticText content = "Kind :" style = "Heading 3"/>
			<StringPropertyText propertyText = "kind"/>
			<ParagraphBreak/>
		</ValueConditionChecker>
		<ValueConditionChecker property = "verifyMethod" operator = "NOT EQUAL" value = "">
			<StaticText content = "VerifyMethod :" style = "Heading 3"/>
			<StringPropertyText propertyText = "verifyMethod"/>
			<ParagraphBreak/>
		</ValueConditionChecker>
		<ValueConditionChecker property = "risk" operator = "NOT EQUAL" value = "">
			<StaticText content = "Risk :" style = "Heading 3"/>
			<StringPropertyText propertyText = "risk"/>
			<ParagraphBreak/>
		</ValueConditionChecker>
		<ValueConditionChecker property = "status" operator = "NOT EQUAL" value = "">
			<StaticText content = "Status :" style = "Heading 3"/>
			<StringPropertyText propertyText = "status"/>
			<ParagraphBreak/>
		</ValueConditionChecker>
				
		<HasRelationshipChecker flag="true" modelType="Verify" direction="to">
			<StaticText content = "Test Cases: " style = "Heading 3"/>
			<ForEachSimpleRelationship type = "Verify" ignoreParagraphBreakForLastModel = "true">
				<MetaModelElement property="from">
					<ValueConditionChecker property="modelType" operator="equals" value="TestCase">
						<StringPropertyText propertyText="name"/>
						<StaticText content = "," />
					</ValueConditionChecker>
				</MetaModelElement>
			</ForEachSimpleRelationship>
			<ParagraphBreak/>
		</HasRelationshipChecker>
		
		<HasRelationshipChecker flag="true" modelType="Association" direction="from">
			<StaticText content = "Composite: " style = "Heading 3"/>
			<ForEachEndRelationship type = "Association" direction="from" ignoreParagraphBreakForLastModel = "true" >
				<MetaModelElement property="endRelationshipToMetaModelElement">
					<ValueConditionChecker property="modelType" operator="equals" value="Requirement">
							<StringPropertyText propertyText="name"/>
							<StaticText content = "," />
					</ValueConditionChecker>
				</MetaModelElement>
			</ForEachEndRelationship>
			<ParagraphBreak/>
		</HasRelationshipChecker>
		
		<HasRelationshipChecker flag="true" modelType="RequirementDerive" direction="from">
			<StaticText content = "Derive: " style = "Heading 3"/>
			<ForEachSimpleRelationship type = "RequirementDerive" ignoreParagraphBreakForLastModel = "true" direction="from">
				<MetaModelElement property="to">
					<ValueConditionChecker property="modelType" operator="equals" value="Requirement">
						<StringPropertyText propertyText="name"/>
						<StaticText content = "," />
					</ValueConditionChecker>
				</MetaModelElement>
			</ForEachSimpleRelationship>
			<ParagraphBreak/>
		</HasRelationshipChecker>
		
		<HasRelationshipChecker flag="true" modelType="RQRefine" direction="from">
			<StaticText content = "Refine: " style = "Heading 3"/>
			<ForEachSimpleRelationship type = "RQRefine" ignoreParagraphBreakForLastModel = "true" direction="from">
				<MetaModelElement property="to">
					<ValueConditionChecker property="modelType" operator="equals" value="Requirement">
						<StringPropertyText propertyText="name"/>
						<StaticText content = "," />
					</ValueConditionChecker>
				</MetaModelElement>
			</ForEachSimpleRelationship>
			<ParagraphBreak/>
		</HasRelationshipChecker>
		
		<HasRelationshipChecker flag="true" modelType="RQTrace" direction="from">
			<StaticText content = "Trace: " style = "Heading 3"/>
			<ForEachSimpleRelationship type = "RQTrace" ignoreParagraphBreakForLastModel = "true" direction="from">
				<MetaModelElement property="to">
					<ValueConditionChecker property="modelType" operator="equals" value="Requirement">
						<StringPropertyText propertyText="name"/>
						<StaticText content = "," />
					</ValueConditionChecker>
				</MetaModelElement>
			</ForEachSimpleRelationship>
			<ParagraphBreak/>
		</HasRelationshipChecker>
		
	</ElementBaseInitiationBlock>	
</Template>