<?xml version="1.0" encoding="UTF-8"?>
<Template>
	<TemplateInformationHeader name="Test Cases" description="/" icon="test plans" type="table" default="false"/>
	<DiagramBaseInitiationBlock>

		<StaticText content="Test Cases" style="Heading 2 (VP)"/>
		<ParagraphBreak/>
		<HasChildElementChecker modelType="TestCase">
			<TableBlock colWidths="30, 70" tableWidth="14500" alignment="right">
				<TableRow>
					<TableCell leftBorderEnable="false" rightBorderEnable="false" color="230, 230, 230">
						<StaticText content="Test Case" style="Column header 1"/>
					</TableCell>
					<TableCell leftBorderEnable="false" rightBorderEnable="false" color="230, 230, 230" splitted="true">
						<TableRow>
							<TableCell leftBorderEnable="false" rightBorderEnable="false" color="230, 230, 230">
								<StaticText content="Steps" style="Column header 1"/>
							</TableCell>
							<TableCell leftBorderEnable="false" rightBorderEnable="false" color="230, 230, 230">
								<StaticText content="Expected Results" style="Column header 1"/>
							</TableCell>
						
						</TableRow>
					</TableCell>
				</TableRow>
				<IterationBlock>
					<ValueConditionChecker property="modelType" operator="equals" value="TestCase">
						<TableRow>
							<TableCell leftBorderEnable="false" rightBorderEnable="false">
								<StringPropertyText propertyText="name" style="Table Contents"/>
							</TableCell>
							<TableCell leftBorderEnable="false" rightBorderEnable="false" splitted="true">
								<TestPlanStepIterationBlock>
									<TableRow>
										<TableCell leftBorderEnable="false" rightBorderEnable="false">
											<StringPropertyText propertyText="index" style="Table Contents"/>
											<StaticText content=" " style="Table Contents"/>
											<StringPropertyText propertyText="name" style="Table Contents"/>
										</TableCell>
										<TableCell leftBorderEnable="false" rightBorderEnable="false">
											<StringPropertyText propertyText="expectedResults" style="Table Contents"/>
										</TableCell>
									</TableRow>
								</TestPlanStepIterationBlock>
							</TableCell>
						</TableRow>
					</ValueConditionChecker>
				</IterationBlock>
			</TableBlock>
		</HasChildElementChecker>
		<StaticText content="" style="Default"/>
	</DiagramBaseInitiationBlock>
</Template>