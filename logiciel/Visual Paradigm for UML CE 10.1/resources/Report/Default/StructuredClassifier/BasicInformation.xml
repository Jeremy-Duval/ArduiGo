<?xml version="1.0" encoding="UTF-8"?>
<Template>
	<TemplateInformationHeader name = "Basic Information" description = "/" type = "table" default = "true"/>
	<ElementBaseInitiationBlock>
	
		<StaticText content = "Class : " style = "Heading 1 (VP)"/>
		<StringPropertyText propertyText = "name" style = "Heading 1 (VP)"/>
		<ParagraphBreak/>
		<TableBlock colWidths = "20, 80" tableWidth = "15500" alignment = "right">
			<ValueConditionChecker property = "extends" operator = "NOT EQUAL" value = "">
				<TableRow>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
						<StaticText content = "Extends :" style = "Row caption 1"/>
					</TableCell>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
						<StringPropertyText propertyText = "extends" style = "Table Contents"/>
					</TableCell>
				</TableRow>
			</ValueConditionChecker>
			<ValueConditionChecker property = "implements" operator = "NOT EQUAL" value = "">
				<TableRow>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
						<StaticText content = "Implements :" style = "Row caption 1"/>
					</TableCell>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
						<StringPropertyText propertyText = "implements" style = "Table Contents"/>
					</TableCell>
				</TableRow>
			</ValueConditionChecker>
			<ValueConditionChecker property = "invariants" operator = "NOT EQUAL" value = "">
				<TableRow>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
						<StaticText content = "Invariants :" style = "Row caption 1"/>
					</TableCell>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
						<StringPropertyText propertyText = "invariants" style = "Table Contents"/>
					</TableCell>
				</TableRow>
			</ValueConditionChecker>
			<HasMetaModelPropertyChecker property = "stereotypes" flag = "true">
				<TableRow>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
						<StaticText content = "Stereotypes :" style = "Row caption 1"/>
					</TableCell>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
						<ForEach property = "stereotypes" ignoreParagraphBreakForLastModel = "true">
							<StaticText content = "&lt;&lt;" style = "Table Contents"/>
							<StringPropertyText propertyText = "name" style = "Table Contents"/>
							<StaticText content = ">>" style = "Table Contents"/>
							<StaticText content = "\n" style = "Table Contents"/>
						</ForEach>
					</TableCell>
				</TableRow>
			</HasMetaModelPropertyChecker>
			<ValueConditionChecker property = "documentation" operator = "NOT EQUAL" value = "">
				<ReportOptionChecker option = "basicInformationShowsDocumentation" equalFlag = "true">
					<TableRow>
						<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
							<StaticText content = "Doc :" style = "Row caption 1"/>
						</TableCell>
						<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
							<StringPropertyText propertyText = "documentation" style = "Table Contents"/>
						</TableCell>
					</TableRow>
				</ReportOptionChecker>
			</ValueConditionChecker>
			<DefaultConditionChecker property = "public" propertyType = "boolean" equalFlag = "false">
				<TableRow>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
						<StaticText content = "Public :" style = "Row caption 1"/>
					</TableCell>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
						<BooleanPropertyText propertyText = "public" style = "Table Contents"/>
					</TableCell>
				</TableRow>
			</DefaultConditionChecker>
			<DefaultConditionChecker property = "final" propertyType = "boolean" equalFlag = "false">
				<TableRow>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
						<StaticText content = "Final :" style = "Row caption 1"/>
					</TableCell>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
						<BooleanPropertyText propertyText = "final" style = "Table Contents"/>
					</TableCell>
				</TableRow>
			</DefaultConditionChecker>
			<DefaultConditionChecker property = "abstract" propertyType = "boolean" equalFlag = "false">
				<TableRow>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
						<StaticText content = "Abstract :" style = "Row caption 1"/>
					</TableCell>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
						<BooleanPropertyText propertyText = "abstract" style = "Table Contents"/>
					</TableCell>
				</TableRow>
			</DefaultConditionChecker>
			<DefaultConditionChecker property = "persistent" propertyType = "boolean" equalFlag = "false">
				<TableRow>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
						<StaticText content = "Persistent :" style = "Row caption 1"/>
					</TableCell>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
						<BooleanPropertyText propertyText = "persistent" style = "Table Contents"/>
					</TableCell>
				</TableRow>
			</DefaultConditionChecker>
		</TableBlock>
		<StaticText content = "" style = "Default"/>
	</ElementBaseInitiationBlock>
</Template>