<Subtemplate>
	<HasMetaModelPropertyChecker property = "trigger" flag= "true">
		<TableRow>
			<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
				<StaticText content = "Trigger :" style = "Row caption 1"/>
			</TableCell>
			<TableCell splitted = "true">
				<MetaModelElement modelProperty = "trigger">
					<TableRow>
						<TableCell leftBorderEnable = "false" rightBorderEnable = "false" color = "230, 230, 230">
							<StaticText content = "Name :" style = "Sub-row caption 1"/>
						</TableCell>
						<TableCell leftBorderEnable = "false" rightBorderEnable = "false" color = "230, 230, 230">
							<StringPropertyText propertyText = "name" style = "Table Contents"/>
						</TableCell>
					</TableRow>			
					<HasMetaModelPropertyChecker property = "eventType" flag= "true">
						<TableRow>
							<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
								<StaticText content = "Event Type :" style = "Sub-row caption 1"/>
							</TableCell>
							<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
								<MetaModelElement modelProperty = "eventType">
									<StringPropertyText propertyText = "name" style = "Table Contents"/>
								</MetaModelElement>
							</TableCell>
						</TableRow>
					</HasMetaModelPropertyChecker>
					<HasMetaModelPropertyChecker property = "stereotypes" flag = "true">
						<TableRow>
							<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
								<StaticText content = "Stereotypes :" style = "Sub-row caption 1"/>
							</TableCell>
							<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
								<IterationBlock elementType = "stereotypes" ignoreParagraphBreakForLastModel = "true">
									<StaticText content = "&lt;&lt;" style = "Table Contents"/>
									<StringPropertyText propertyText = "name" style = "Table Contents"/>
									<StaticText content = ">>" style = "Table Contents"/>
									<StaticText content = "\n" style = "Table Contents"/>
								</IterationBlock>
							</TableCell>
						</TableRow>
					</HasMetaModelPropertyChecker>
					<ValueConditionChecker property = "documentation" operator = "NOT EQUAL" value = "">
						<TableRow>
							<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
								<StaticText content = "Documentation :" style = "Sub-row caption 1"/>
							</TableCell>
							<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
								<StringPropertyText propertyText = "documentation" style = "Table Contents"/>
							</TableCell>
						</TableRow>
					</ValueConditionChecker>
					<HasChildElementChecker type = "Parameter" flag = "true">
						<TableRow>
							<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
								<StaticText content = "Parameters :" style = "Sub-row caption 1"/>
							</TableCell>
							<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
								<IterationBlock elementType = "Parameter">
									<StringPropertyText propertyText = "name" style = "Table Contents"/>
									<StaticText content = " : " style = "Table Contents"/>
									<StringPropertyText propertyText = "type" style = "Table Contents"/>
									<StaticText content = "\n" style = "Table Contents"/>
								</IterationBlock>
							</TableCell>
						</TableRow>
					</HasChildElementChecker>
				</MetaModelElement>								
			</TableCell>
		</TableRow>
	</HasMetaModelPropertyChecker>
	
</Subtemplate>