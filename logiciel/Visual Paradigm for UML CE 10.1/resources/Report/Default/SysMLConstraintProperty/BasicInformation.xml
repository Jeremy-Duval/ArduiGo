<?xml version="1.0" encoding="UTF-8"?>
<Template>
	<TemplateInformationHeader name="Basic Information" description="/" type="table" default="true"/>
	<ElementBaseInitiationBlock>
	
		<StringPropertyText propertyText="modelType" style="Heading 1 (VP)"/>
		<StaticText content=" : " style="Heading 1 (VP)"/>
		<StringPropertyText propertyText="name" style="Heading 1 (VP)"/>
		<ParagraphBreak/>
		
		<TableBlock colWidths = "20, 80" tableWidth="15500" alignment="right">
			
			<!-- stereotypes -->
			<HasMetaModelPropertyChecker property = "stereotypes" flag = "true">
				<TableRow>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
						<StaticText content = "Stereotypes :" style = "Row caption 1"/>
					</TableCell>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
						<ForEach property = "stereotypes" ignoreParagraphBreakForLastModel = "true">
							<StaticText content = "&lt;&lt;" style = "Table Contents"/>
							<StringPropertyText propertyText = "name" style = "Table Contents"/>
							<StaticText content = ">>" style = "Table Contents"/>
							<StaticText content = "\n" style = "Table Contents"/>
						</ForEach>
					</TableCell>
				</TableRow>
			</HasMetaModelPropertyChecker>
			
			<!-- multiplicity -->
			<ValueConditionChecker property="multiplicity" operator="NOT EQUAL" value="Unspecified">
				<TableRow>
					<TableCell leftBorderEnable="false" rightBorderEnable="false">
						<StaticText content="Multiplicity :" style="Row caption 1"/>
					</TableCell>
					<TableCell leftBorderEnable="false" rightBorderEnable="false">
						<StringPropertyText propertyText="multiplicity" style="Table Contents"/>
					</TableCell>
				</TableRow>
			</ValueConditionChecker>
			
			<!-- multiplicity details -->
			<HasMetaModelPropertyChecker property="multiplicityDetail" flag= "true">
				<MetaModelElement modelProperty="multiplicityDetail">
					<TableRow>
						<TableCell leftBorderEnable="false" rightBorderEnable="false">
							<StaticText content="Multiplicity detail :" style="Row caption 1"/>
						</TableCell>
						<TableCell splitted="true">	
							<DefaultConditionChecker property="ordered" propertyType="boolean" equalFlag="false">
								<TableRow>
									<TableCell leftBorderEnable="false" rightBorderEnable="false">
										<StaticText content="Ordered :" style="Sub-row caption 1"/>
									</TableCell>
									<TableCell leftBorderEnable="false" rightBorderEnable="false">
										<BooleanPropertyText propertyText="ordered" style="Table Contents"/>
									</TableCell>
								</TableRow>
							</DefaultConditionChecker>
							<DefaultConditionChecker property="unique" propertyType="boolean" equalFlag="false">
								<TableRow>
									<TableCell leftBorderEnable="false" rightBorderEnable="false">
										<StaticText content="Unique :" style="Sub-row caption 1"/>
									</TableCell>
									<TableCell leftBorderEnable="false" rightBorderEnable="false">
										<BooleanPropertyText propertyText="unique" style="Table Contents"/>
									</TableCell>
								</TableRow>
							</DefaultConditionChecker>							
						</TableCell>
					</TableRow>
				</MetaModelElement>
			</HasMetaModelPropertyChecker>
			
			<!-- visibility -->
			<DefaultConditionChecker property = "visibility" propertyType = "StringSelection" equalFlag = "false">
				<TableRow>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
						<StaticText content = "Visibility :" style = "Row caption 1"/>
					</TableCell>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
						<StringPropertyText propertyText = "visibility" style = "Table Contents"/>
					</TableCell>
				</TableRow>
			</DefaultConditionChecker>
			
			<!-- documentation -->
			<ValueConditionChecker property="documentation" operator="NOT EQUAL" value="">
				<ReportOptionChecker option="basicInformationShowsDocumentation" equalFlag="true">
					<TableRow>
						<TableCell leftBorderEnable="false" rightBorderEnable="false">
							<StaticText content="Doc :" style="Row caption 1"/>
						</TableCell>
						<TableCell leftBorderEnable="false" rightBorderEnable="false">
							<StringPropertyText propertyText="documentation" style="Table Contents"/>
						</TableCell>
					</TableRow>
				</ReportOptionChecker>
			</ValueConditionChecker>
			
			<!-- defaultValue -->
			<DefaultConditionChecker property="defaultValue" flag="false">
				<TableRow>
					<TableCell leftBorderEnable="false" rightBorderEnable="false">
						<StaticText content="Default Value :" style="Row caption 1"/>
					</TableCell>
					<TableCell leftBorderEnable="false" rightBorderEnable="false">
						<StringPropertyText property="defaultValue" style="Table Contents"/>
					</TableCell>
				</TableRow>
			</DefaultConditionChecker>
			
			<!-- leaf -->
			<DefaultConditionChecker property="leaf" flag="false">
				<TableRow>
					<TableCell leftBorderEnable="false" rightBorderEnable="false">
						<StaticText content="Leaf :" style="Row caption 1"/>
					</TableCell>
					<TableCell leftBorderEnable="false" rightBorderEnable="false">
						<StringPropertyText property="leaf" style="Table Contents"/>
					</TableCell>
				</TableRow>
			</DefaultConditionChecker>
			
			<!-- static -->
			<DefaultConditionChecker property="static" flag="false">
				<TableRow>
					<TableCell leftBorderEnable="false" rightBorderEnable="false">
						<StaticText content="Static :" style="Row caption 1"/>
					</TableCell>
					<TableCell leftBorderEnable="false" rightBorderEnable="false">
						<StringPropertyText property="static" style="Table Contents"/>
					</TableCell>
				</TableRow>
			</DefaultConditionChecker>
			
			<!-- read only -->
			<DefaultConditionChecker property="readOnly" flag="false">
				<TableRow>
					<TableCell leftBorderEnable="false" rightBorderEnable="false">
						<StaticText content="Read only :" style="Row caption 1"/>
					</TableCell>
					<TableCell leftBorderEnable="false" rightBorderEnable="false">
						<StringPropertyText property="readOnly" style="Table Contents"/>
					</TableCell>
				</TableRow>
			</DefaultConditionChecker>
			
			<!-- derived -->
			<DefaultConditionChecker property="derived" flag="false">
				<TableRow>
					<TableCell leftBorderEnable="false" rightBorderEnable="false">
						<StaticText content="Derived :" style="Row caption 1"/>
					</TableCell>
					<TableCell leftBorderEnable="false" rightBorderEnable="false">
						<StringPropertyText property="derived" style="Table Contents"/>
					</TableCell>
				</TableRow>
			</DefaultConditionChecker>
			
			<!-- derivedUnion -->
			<DefaultConditionChecker property="derivedUnion" flag="false">
				<TableRow>
					<TableCell leftBorderEnable="false" rightBorderEnable="false">
						<StaticText content="Derived Union :" style="Row caption 1"/>
					</TableCell>
					<TableCell leftBorderEnable="false" rightBorderEnable="false">
						<StringPropertyText property="derivedUnion" style="Table Contents"/>
					</TableCell>
				</TableRow>
			</DefaultConditionChecker>
			
			<!-- aggregation -->
			<DefaultConditionChecker property="aggregation" flag="false">
				<TableRow>
					<TableCell leftBorderEnable="false" rightBorderEnable="false">
						<StaticText content="Aggregation :" style="Row caption 1"/>
					</TableCell>
					<TableCell leftBorderEnable="false" rightBorderEnable="false">
						<StringPropertyText property="aggregation" style="Table Contents"/>
					</TableCell>
				</TableRow>
			</DefaultConditionChecker>
			
		</TableBlock>
		<StaticText content="" style="Default"/>
	</ElementBaseInitiationBlock>
</Template>