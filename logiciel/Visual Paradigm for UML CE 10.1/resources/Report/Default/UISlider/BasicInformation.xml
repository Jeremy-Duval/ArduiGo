<?xml version="1.0" encoding="UTF-8"?>
<Template>
	<TemplateInformationHeader name = "Basic Information" description = "/" type = "table" default = "true"/>
	<ElementBaseInitiationBlock>
	
		<StringPropertyText propertyText = "modelType" style = "Heading 1 (VP)"/>
		<StaticText content = " : " style = "Heading 1 (VP)"/>
		<StringPropertyText propertyText = "name" style = "Heading 1 (VP)"/>
		<ParagraphBreak/>
		
		<TableBlock colWidths = "20, 80" tableWidth = "15500" alignment = "right">
			
			<!-- orientation -->
			<DefaultConditionChecker property = "orientation" propertyType = "int" equalFlag = "false">
				<TableRow>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
						<StaticText content = "Orientation :" style = "Row caption 1"/>
					</TableCell>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
						<StringPropertyText propertyText = "orientation" style = "Table Contents"/>
					</TableCell>
				</TableRow>
			</DefaultConditionChecker>
			
			<!-- minimum -->
			<DefaultConditionChecker property = "minimum" propertyType = "int" equalFlag = "false">
				<TableRow>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
						<StaticText content = "Minimum :" style = "Row caption 1"/>
					</TableCell>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
						<StringPropertyText propertyText = "minimum" style = "Table Contents"/>
					</TableCell>
				</TableRow>
			</DefaultConditionChecker>
			
			<!-- maximum -->
			<DefaultConditionChecker property = "maximum" propertyType = "int" equalFlag = "false">
				<TableRow>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
						<StaticText content = "Maximum :" style = "Row caption 1"/>
					</TableCell>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
						<StringPropertyText propertyText = "maximum" style = "Table Contents"/>
					</TableCell>
				</TableRow>
			</DefaultConditionChecker>
			
			<!-- value -->
			<DefaultConditionChecker property = "value" propertyType = "int" equalFlag = "false">
				<TableRow>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
						<StaticText content = "Value :" style = "Row caption 1"/>
					</TableCell>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
						<StringPropertyText propertyText = "value" style = "Table Contents"/>
					</TableCell>
				</TableRow>
			</DefaultConditionChecker>
			
			<!-- minorTickSpacing -->
			<DefaultConditionChecker property = "minorTickSpacing" propertyType = "int" equalFlag = "false">
				<TableRow>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
						<StaticText content = "Minor Tick Spacing :" style = "Row caption 1"/>
					</TableCell>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
						<StringPropertyText propertyText = "minorTickSpacing" style = "Table Contents"/>
					</TableCell>
				</TableRow>
			</DefaultConditionChecker>
			
			<!-- majorTickSpacing -->
			<DefaultConditionChecker property = "majorTickSpacing" propertyType = "int" equalFlag = "false">
				<TableRow>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
						<StaticText content = "Major Tick Spacing :" style = "Row caption 1"/>
					</TableCell>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
						<StringPropertyText propertyText = "majorTickSpacing" style = "Table Contents"/>
					</TableCell>
				</TableRow>
			</DefaultConditionChecker>
			
			<!-- paintTicks -->
			<DefaultConditionChecker property = "paintTicks" propertyType = "boolean" equalFlag = "false">
				<TableRow>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
						<StaticText content = "Paint Ticks :" style = "Row caption 1"/>
					</TableCell>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
						<StringPropertyText propertyText = "paintTicks" style = "Table Contents"/>
					</TableCell>
				</TableRow>
			</DefaultConditionChecker>
			
			<!-- paintTrack -->
			<DefaultConditionChecker property = "paintTrack" propertyType = "boolean" equalFlag = "false">
				<TableRow>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
						<StaticText content = "Paint Track :" style = "Row caption 1"/>
					</TableCell>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
						<StringPropertyText propertyText = "paintTrack" style = "Table Contents"/>
					</TableCell>
				</TableRow>
			</DefaultConditionChecker>
			
			<!-- paintLabels -->
			<DefaultConditionChecker property = "paintLabels" propertyType = "boolean" equalFlag = "false">
				<TableRow>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
						<StaticText content = "Paint Labels :" style = "Row caption 1"/>
					</TableCell>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
						<StringPropertyText propertyText = "paintLabels" style = "Table Contents"/>
					</TableCell>
				</TableRow>
			</DefaultConditionChecker>
			
			<!-- documentation -->
			<ValueConditionChecker property = "documentation" operator = "NOT EQUAL" value = "">
				<ReportOptionChecker option = "basicInformationShowsDocumentation" equalFlag = "true">
					<TableRow>
						<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
							<StaticText content = "Doc :" style = "Row caption 1"/>
						</TableCell>
						<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
							<StringPropertyText propertyText = "documentation" style = "Table Contents"/>
						</TableCell>
					</TableRow>
				</ReportOptionChecker>
			</ValueConditionChecker>
			
		</TableBlock>
		<StaticText content = "" style = "Default"/>
	</ElementBaseInitiationBlock>
</Template>