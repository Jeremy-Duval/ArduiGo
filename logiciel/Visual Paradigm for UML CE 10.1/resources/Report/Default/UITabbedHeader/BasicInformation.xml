<?xml version="1.0" encoding="UTF-8"?>
<Template>
	<TemplateInformationHeader name = "Basic Information" description = "/" type = "table" default = "true"/>
	<ElementBaseInitiationBlock>
	
		<StringPropertyText propertyText = "modelType" style = "Heading 1 (VP)"/>
		<StaticText content = " : " style = "Heading 1 (VP)"/>
		<StringPropertyText propertyText = "name" style = "Heading 1 (VP)"/>
		<ParagraphBreak/>
		
		<TableBlock colWidths = "20, 80" tableWidth = "15500" alignment = "right">
			
			<!-- tabPlacement -->
			<DefaultConditionChecker property = "tabPlacement" propertyType = "int" equalFlag = "false">
				<TableRow>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
						<StaticText content = "Tab Placement :" style = "Row caption 1"/>
					</TableCell>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
						<StringPropertyText propertyText = "tabPlacement" style = "Table Contents"/>
					</TableCell>
				</TableRow>
			</DefaultConditionChecker>
			
			<!-- tabLayoutPolicy -->
			<DefaultConditionChecker property = "tabLayoutPolicy" propertyType = "int" equalFlag = "false">
				<TableRow>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
						<StaticText content = "Tab Layout Policy :" style = "Row caption 1"/>
					</TableCell>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
						<StringPropertyText propertyText = "tabLayoutPolicy" style = "Table Contents"/>
					</TableCell>
				</TableRow>
			</DefaultConditionChecker>
			
			<!-- tabs -->
			<HasMetaModelPropertyChecker property = "tabs" flag = "true">
				<TableRow>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
						<StaticText content = "Tabs :" style = "Row caption 1"/>
					</TableCell>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
						<ForEach property="tabs" ignoreParagraphBreakForLastModel="true">
							<HasMetaModelPropertyChecker property="value" flag="true">
								<MetaModelElement modelProperty="value">
									<StringPropertyText propertyText="name" style="Table Contents"/>
								</MetaModelElement>
							</HasMetaModelPropertyChecker>
							<HasMetaModelPropertyChecker property="value" flag="false">
								<StringPropertyText propertyText="value" style="Table Contents"/>
							</HasMetaModelPropertyChecker>
							<StaticText content="\n" style="Table Contents"/>
						</ForEach>
					</TableCell>
				</TableRow>
			</HasMetaModelPropertyChecker>
			
			<!-- tabsDisplayedMnemonic -->
			<HasMetaModelPropertyChecker property = "tabsDisplayedMnemonic" flag = "true">
				<TableRow>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
						<StaticText content = "Tabs DisplayedMnemonic :" style = "Row caption 1"/>
					</TableCell>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
						<ForEach property="tabsDisplayedMnemonic" ignoreParagraphBreakForLastModel="true">
							<HasMetaModelPropertyChecker property="value" flag="true">
								<MetaModelElement modelProperty="value">
									<StringPropertyText propertyText="name" style="Table Contents"/>
								</MetaModelElement>
							</HasMetaModelPropertyChecker>
							<HasMetaModelPropertyChecker property="value" flag="false">
								<StringPropertyText propertyText="value" style="Table Contents"/>
							</HasMetaModelPropertyChecker>
							<StaticText content="\n" style="Table Contents"/>
						</ForEach>
					</TableCell>
				</TableRow>
			</HasMetaModelPropertyChecker>
			
			<!-- selected tab -->
			<ValueConditionChecker property = "selectedTab" operator = "NOT EQUAL" value = "">
				<TableRow>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
						<StaticText content = "Selected Tab :" style = "Row caption 1"/>
					</TableCell>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
						<StringPropertyText propertyText = "selectedTab" style = "Table Contents"/>
					</TableCell>
				</TableRow>
			</ValueConditionChecker>
			
			<!-- documentation -->
			<ValueConditionChecker property = "documentation" operator = "NOT EQUAL" value = "">
				<ReportOptionChecker option = "basicInformationShowsDocumentation" equalFlag = "true">
					<TableRow>
						<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
							<StaticText content = "Doc :" style = "Row caption 1"/>
						</TableCell>
						<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
							<StringPropertyText propertyText = "documentation" style = "Table Contents"/>
						</TableCell>
					</TableRow>
				</ReportOptionChecker>
			</ValueConditionChecker>
			
		</TableBlock>
		<StaticText content = "" style = "Default"/>
	</ElementBaseInitiationBlock>
</Template>