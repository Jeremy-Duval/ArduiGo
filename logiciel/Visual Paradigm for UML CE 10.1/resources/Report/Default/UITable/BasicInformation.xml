<?xml version="1.0" encoding="UTF-8"?>
<Template>
	<TemplateInformationHeader name = "Basic Information" description = "/" type = "table" default = "true"/>
	<ElementBaseInitiationBlock>
	
		<StringPropertyText propertyText = "modelType" style = "Heading 1 (VP)"/>
		<StaticText content = " : " style = "Heading 1 (VP)"/>
		<StringPropertyText propertyText = "name" style = "Heading 1 (VP)"/>
		<ParagraphBreak/>
		
		<TableBlock colWidths = "20, 80" tableWidth = "15500" alignment = "right">
			
			<!-- columns -->
			<HasMetaModelPropertyChecker property = "columns" flag = "true">
				<TableRow>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
						<StaticText content = "Columns :" style = "Row caption 1"/>
					</TableCell>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
						<ForEach property="columns" ignoreParagraphBreakForLastModel="true">
							<StringPropertyText propertyText="name" style="Table Contents"/>
							<StaticText content="\n" style="Table Contents"/>
						</ForEach>
					</TableCell>
				</TableRow>
			</HasMetaModelPropertyChecker>
			
			<!-- horizontalScrollBarPolicy -->
			<DefaultConditionChecker property = "horizontalScrollBarPolicy" propertyType = "int" equalFlag = "false">
				<TableRow>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
						<StaticText content = "Horizontal Scroll Bar Policy :" style = "Row caption 1"/>
					</TableCell>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
						<StringPropertyText propertyText = "horizontalScrollBarPolicy" style = "Table Contents"/>
					</TableCell>
				</TableRow>
			</DefaultConditionChecker>
			
			<!-- verticalScrollBarPolicy -->
			<DefaultConditionChecker property = "verticalScrollBarPolicy" propertyType = "int" equalFlag = "false">
				<TableRow>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
						<StaticText content = "Vertical Scroll Bar Policy :" style = "Row caption 1"/>
					</TableCell>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
						<StringPropertyText propertyText = "verticalScrollBarPolicy" style = "Table Contents"/>
					</TableCell>
				</TableRow>
			</DefaultConditionChecker>
			
			<!-- rowHeight -->
			<DefaultConditionChecker property = "rowHeight" propertyType = "int" equalFlag = "false">
				<TableRow>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
						<StaticText content = "Row Height :" style = "Row caption 1"/>
					</TableCell>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
						<StringPropertyText propertyText = "rowHeight" style = "Table Contents"/>
					</TableCell>
				</TableRow>
			</DefaultConditionChecker>
			
			<!-- gridColorValue -->
			<DefaultConditionChecker property = "gridColorValue" propertyType = "int" equalFlag = "false">
				<TableRow>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
						<StaticText content = "Grid Color Value :" style = "Row caption 1"/>
					</TableCell>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
						<StringPropertyText propertyText = "gridColorValue" style = "Table Contents"/>
					</TableCell>
				</TableRow>
			</DefaultConditionChecker>
			
			<!-- showHorizontalLines -->
			<DefaultConditionChecker property = "showHorizontalLines" propertyType = "boolean" equalFlag = "false">
				<TableRow>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
						<StaticText content = "Show Horizontal Lines :" style = "Row caption 1"/>
					</TableCell>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
						<StringPropertyText propertyText = "showHorizontalLines" style = "Table Contents"/>
					</TableCell>
				</TableRow>
			</DefaultConditionChecker>
			
			<!-- showVerticalLines -->
			<DefaultConditionChecker property = "showVerticalLines" propertyType = "boolean" equalFlag = "false">
				<TableRow>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
						<StaticText content = "Show Vertical Lines :" style = "Row caption 1"/>
					</TableCell>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
						<StringPropertyText propertyText = "showVerticalLines" style = "Table Contents"/>
					</TableCell>
				</TableRow>
			</DefaultConditionChecker>
			
			<!-- autoResizeMode -->
			<DefaultConditionChecker property = "autoResizeMode" propertyType = "int" equalFlag = "false">
				<TableRow>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
						<StaticText content = "Auto Resize Mode :" style = "Row caption 1"/>
					</TableCell>
					<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
						<StringPropertyText propertyText = "autoResizeMode" style = "Table Contents"/>
					</TableCell>
				</TableRow>
			</DefaultConditionChecker>
			
			<!-- documentation -->
			<ValueConditionChecker property = "documentation" operator = "NOT EQUAL" value = "">
				<ReportOptionChecker option = "basicInformationShowsDocumentation" equalFlag = "true">
					<TableRow>
						<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
							<StaticText content = "Doc :" style = "Row caption 1"/>
						</TableCell>
						<TableCell leftBorderEnable = "false" rightBorderEnable = "false">
							<StringPropertyText propertyText = "documentation" style = "Table Contents"/>
						</TableCell>
					</TableRow>
				</ReportOptionChecker>
			</ValueConditionChecker>
			
		</TableBlock>
		<StaticText content = "" style = "Default"/>
	</ElementBaseInitiationBlock>
</Template>