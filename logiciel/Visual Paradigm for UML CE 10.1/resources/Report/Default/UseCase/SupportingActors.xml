<?xml version="1.0" encoding="UTF-8"?>
<Template>
	<TemplateInformationHeader name="Supporting Actors" description="/" type="table" default="false"/>
	<ElementBaseInitiationBlock>
		<StaticText content="Supporting Actors" style="Heading 2 (VP)"/>
		<ParagraphBreak/>
		<HasMetaModelPropertyChecker property="supportingActors" flag="true">
			<TableBlock colWidths="100" tableWidth="14500" alignment="right">
				<TableRow>
					<TableCell leftBorderEnable="false" rightBorderEnable="false" color="230, 230, 230">
						<StaticText content="Name" style="Column header 1"/>
					</TableCell>
				</TableRow>
				<ForEach property="supportingActors" ignoreParagraphBreakForLastModel="false">
					<TableRow>
						<TableCell leftBorderEnable="false" rightBorderEnable="false">
							<StringPropertyText propertyText="name" style="Table Contents"/>
						</TableCell>
					</TableRow>
				</ForEach>
			</TableBlock>
		</HasMetaModelPropertyChecker>
		<StaticText content="" style="Default"/>
	</ElementBaseInitiationBlock>
</Template>