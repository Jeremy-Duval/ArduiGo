<Template>
	<TemplateInformationHeader name = "UseCase" description = "/" type = "text" default = "false"/>
	<ElementBaseInitiationBlock>
		
		<ElementImage imageType="icon"/>
		<StringPropertyText propertyText="name" style = "Heading 1"/>
		<ParagraphBreak/>
		
		<ValueConditionChecker property="documentation" operator="not equals" value="">
			<StringPropertyText propertyText="documentation"/>
			<ParagraphBreak/>
		</ValueConditionChecker>
	
		<HasRelationshipChecker flag="true" modelType="Association">
			<StaticText content = "Actors: " style = "Heading 3"/>
			<ForEachRelationshipEnd type = "AssociationEnd" endPointer = "self" ignoreParagraphBreakForLastModel = "true">
				<RelationshipEndEndRelationship>
					<FromEnd>
						<MetaModelElement property="EndModelElement">
							<ValueConditionChecker property="modelType" operator="equals" value="Actor">
								<StringPropertyText propertyText="name"/>
								<StaticText content = "," />
							</ValueConditionChecker>
						</MetaModelElement>
					</FromEnd>
					<ToEnd>
						<MetaModelElement property="EndModelElement">
							<ValueConditionChecker property="modelType" operator="equals" value="Actor">
								<StringPropertyText propertyText="name"/>
								<StaticText content = "," />
							</ValueConditionChecker>
						</MetaModelElement>
					</ToEnd>
				</RelationshipEndEndRelationship>
			</ForEachRelationshipEnd>
			<ParagraphBreak/>
		</HasRelationshipChecker>
		
		<HasMetaModelPropertyChecker property="supportingActors" flag="true">
			<StaticText content = "Supporting Actors: " style = "Heading 3"/>
			<ForEach property="supportingActors" ignoreParagraphBreakForLastModel = "true">
				<StringPropertyText propertyText="name"/>
				<StaticText content = "," />
			</ForEach>
			<ParagraphBreak/>
		</HasMetaModelPropertyChecker>
		
		<HasRelationshipChecker flag="true" modelType="Include" direction="from">
			<StaticText content = "Include: " style = "Heading 3"/>
			<ForEachSimpleRelationship type = "Include" ignoreParagraphBreakForLastModel = "true">
				<MetaModelElement property="to">
					<ValueConditionChecker property="modelType" operator="equals" value="UseCase">
						<StringPropertyText propertyText="name"/>
						<StaticText content = "," />
					</ValueConditionChecker>
				</MetaModelElement>
			</ForEachSimpleRelationship>
			<ParagraphBreak/>
		</HasRelationshipChecker>
		
		<HasRelationshipChecker flag="true" modelType="Include" direction="to">
			<StaticText content = "Include by: " style = "Heading 3"/>
			<ForEachSimpleRelationship type = "Include" ignoreParagraphBreakForLastModel = "true">
				<MetaModelElement property="from">
					<ValueConditionChecker property="modelType" operator="equals" value="UseCase">
						<StringPropertyText propertyText="name"/>
						<StaticText content = "," />
					</ValueConditionChecker>
				</MetaModelElement>
			</ForEachSimpleRelationship>
			<ParagraphBreak/>
		</HasRelationshipChecker>
		
		<HasRelationshipChecker flag="true" modelType="Extend" direction="from">
			<StaticText content = "Extend: " style = "Heading 3"/>
			<ForEachSimpleRelationship type = "Extend" ignoreParagraphBreakForLastModel = "true">
				<MetaModelElement property="to">
					<ValueConditionChecker property="modelType" operator="equals" value="UseCase">
						<StringPropertyText propertyText="name"/>
						<StaticText content = "," />
					</ValueConditionChecker>
				</MetaModelElement>
			</ForEachSimpleRelationship>
			<ParagraphBreak/>
		</HasRelationshipChecker>
		
		<HasRelationshipChecker flag="true" modelType="Extend" direction="to">
			<StaticText content = "Extend from: " style = "Heading 3"/>
			<ForEachSimpleRelationship type = "Extend" ignoreParagraphBreakForLastModel = "true">
				<MetaModelElement property="from">
					<ValueConditionChecker property="modelType" operator="equals" value="UseCase">
						<StringPropertyText propertyText="name"/>
						<StaticText content = "," />
					</ValueConditionChecker>
				</MetaModelElement>
			</ForEachSimpleRelationship>
			<ParagraphBreak/>
		</HasRelationshipChecker>
		
		<HasRelationshipChecker flag="true" modelType="Generalization" direction="to">
			<StaticText content = "Super Use Cases: " style = "Heading 3"/>
			<ForEachSimpleRelationship type = "Generalization" ignoreParagraphBreakForLastModel = "true">
				<MetaModelElement property="from">
					<ValueConditionChecker property="modelType" operator="equals" value="UseCase">
						<StringPropertyText propertyText="name"/>
						<StaticText content = "," />
					</ValueConditionChecker>
				</MetaModelElement>
			</ForEachSimpleRelationship>
			<ParagraphBreak/>
		</HasRelationshipChecker>
		
		<HasRelationshipChecker flag="true" modelType="Generalization" direction="from">
			<StaticText content = "Sub Use Cases: " style = "Heading 3"/>
			<ForEachSimpleRelationship type = "Generalization" ignoreParagraphBreakForLastModel = "true">
				<MetaModelElement property="to">
					<ValueConditionChecker property="modelType" operator="equals" value="UseCase">
						<StringPropertyText propertyText="name"/>
						<StaticText content = "," />
					</ValueConditionChecker>
				</MetaModelElement>
			</ForEachSimpleRelationship>
			<ParagraphBreak/>
		</HasRelationshipChecker>
		
		<HasMetaModelPropertyChecker property="stepContainers" flag="true">
			<FlowOfEvents/>
		</HasMetaModelPropertyChecker>
		
		<ForEachSubDiagram diagramType="InteractionDiagram">
			<StringPropertyText propertyText="name"/>		
			<ParagraphBreak/>
			<ElementImage imageType="diagram" alignment="center" keepWithPreviousInPDF="true"/>
			<ParagraphBreak/>
		</ForEachSubDiagram>
		
	</ElementBaseInitiationBlock>	
</Template>