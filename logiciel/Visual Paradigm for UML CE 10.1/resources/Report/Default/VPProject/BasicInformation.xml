<?xml version="1.0" encoding="UTF-8"?>
<Template>
	<TemplateInformationHeader name="Basic Information" description="/" type="table" default="true"/>
	<ProjectBaseInitiationBlock>
	
		<StaticText content="Project : " style="Heading 1 (VP)"/>
		<StringPropertyText propertyText="name" style="Heading 1 (VP)"/>
		<ParagraphBreak/>
		
		<TableBlock colWidths = "20, 80" tableWidth="15500" alignment="right">
			<TableRow>
				<TableCell leftBorderEnable="false" rightBorderEnable="false">
					<StaticText content="Author :" style="Row caption 1"/>
				</TableCell>
				<TableCell leftBorderEnable="false" rightBorderEnable="false">
					<StringPropertyText propertyText="author" style="Table Contents"/>
				</TableCell>
			</TableRow>
			<TableRow>
				<TableCell leftBorderEnable="false" rightBorderEnable="false">
					<StaticText content="Company :" style="Row caption 1"/>
				</TableCell>
				<TableCell leftBorderEnable="false" rightBorderEnable="false">
					<StringPropertyText propertyText="company" style="Table Contents"/>
				</TableCell>
			</TableRow>
			<TableRow>
				<TableCell leftBorderEnable="false" rightBorderEnable="false">
					<StaticText content="Description :" style="Row caption 1"/>
				</TableCell>
				<TableCell leftBorderEnable="false" rightBorderEnable="false">
					<StringPropertyText propertyText="description" style="Table Contents"/>
				</TableCell>
			</TableRow>
			<TableRow>
				<TableCell leftBorderEnable="false" rightBorderEnable="false">
					<StaticText content="UML Version :" style="Row caption 1"/>
				</TableCell>
				<TableCell leftBorderEnable="false" rightBorderEnable="false">
					<StringPropertyText propertyText="umlVersion" style="Table Contents"/>
				</TableCell>
			</TableRow>
		</TableBlock>
		<ParagraphBreak/>
		
		<StaticText content="" style="Default"/>
	</ProjectBaseInitiationBlock>
</Template>